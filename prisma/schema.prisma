generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post{
  id Int @id @default(autoincrement())
  kotitle String?
  kosinger String?
  koyoutube String?
  koalbum String?
  korelease String?
  kothumbnail String?
  kolyrics String?
  kotranslate String?
  jptitle String?
  jpyoutube String?
  jpsinger String?
  jpalbum String?
  jprelease String?
  jpthumbnail String?
  jplyrics String?
  jptranslate String?
  date DateTime @default(now())
}